{ 
  "Version": "2012-10-17", 
  "Statement": [ 
    { 
        "Effect": "Allow", 
        "Action": ["s3:ListBucket"], 
        "Resource": ["arn:aws:s3 :::ap-southeast-2-test"] 
    }, 
    { 
        "Effect": "Allow", 
        "Action": [ 
            "s3:PutObject", 
            "s3:GetObject", 
            "s3:DeleteObject" 
        ], 
        "Resource": ["arn:aws :s3:::ap-southeast-2-test/*"] 
    },
    {
        "Effect": "Allow",
        "Action": [
          "kms:Decrypt"
        ],
        "Resource": "arn:aws:kms:*:12345:key/*"
    }
  ] 
}
